&tlmm {
	gpio-reserved-ranges = <28 4>, <40 4>;

	tert_tdm_active: tert-tdm-active-state {
		sck-pins {
			pins = "gpio133";
			function = "mi2s2_sck";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		din-pins {
			pins = "gpio134";
			function = "mi2s2_data0";
			drive-strength = <8>;
			bias-disable;
		};

		ws-pins {
			pins = "gpio135";
			function = "mi2s2_ws";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		dout-pins {
			pins = "gpio137";
			function = "mi2s2_data1";
			drive-strength = <8>;
			bias-disable;
		};
	};

	bt_en_state: bt-default-state {
		bt-en-pins {
			pins = "gpio21";
			function = "gpio";
			drive-strength = <16>;
			output-low;
			bias-pull-up;
		};
	};

	wlan_en_state: wlan-default-state {
		wlan-en-pins {
			pins = "gpio20";
			function = "gpio";
			drive-strength = <16>;
			output-low;
			bias-pull-up;
		};
	};

	smartpa_int_active: smartpa_int_active {
			pins = "gpio100";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
	};

	smartpa_int_suspend: smartpa_int_suspend {
			pins = "gpio100";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
	};

	smartpa_enable_active: smartpa_enable_active {
			pins = "gpio114";
			function = "gpio";
			drive-strength = <2>;
			bias-disable;
			bias-pull-down;
			output-low;
	};

	smartpa_enable_suspend: smartpa_enable_suspend {
			pins = "gpio114";
			function = "gpio";
			drive-strength = <2>;
			bias-disable;
			bias-pull-down;
			output-low;
	};
	
   wcd_default: wcd-reset-n-active-state {
		pins = "gpio108";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
		output-low;
	};

	tp_active: tp_active {
			pins = "gpio38", "gpio39", "gpio72";
			function = "gpio";
			drive-strength = <16>;
			bias-pull-up;
	};
	
	tp_int_suspend: tp_int_suspend {
			pins = "gpio39";
			function = "gpio";
			drive-strength = <16>;
			input-enable;
			bias-pull-up;
	};

	tp_reset_suspend: tp_reset_suspend {
			pins = "gpio38";
			function = "gpio";
			drive-strength = <16>;
			bias-disable;
	};

	sde_dsi_active: sde_dsi_active-state {
		pins = "gpio46";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	sde_dsi_sleep: sde-dsi-sleep-state {
		pins = "gpio46";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
		output-high;
	};

	sde_te_active_sleep: sde_te_active-state {
		pins = "gpio46";
		function = "mdp_vsync";
		drive-strength = <2>;
		bias-pull-down;
	};
  
   bq2597x_master_int_default: bq2597x-master-int-default {
		pins = "gpio157";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
		input-enable;
	}; 

	sw_ctrl: sw-ctrl-state {
		pins = "gpio124";
		function = "gpio";
		bias-pull-down;
	};
};

